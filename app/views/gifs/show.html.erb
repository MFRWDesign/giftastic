<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @gif.title %>
</p>

<p>
  <strong>User:</strong>
  <%= @gif.user_id %>
</p>

<p>
  <strong>Image:</strong>
  <% if @gif.image.present? %>
    <%= image_tag url_for(@gif.image) %>
  <% end %>
</p>

<%= link_to 'Edit', edit_gif_path(@gif) %> |
<%= link_to 'Back', gifs_path %>
